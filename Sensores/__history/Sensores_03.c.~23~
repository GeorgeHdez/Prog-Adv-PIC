#include <18f4550.h>
#device ADC = 10;
#FUSES XT,NOPROTECT,NOLVP,NOWDT
#use delay(clock = 4M)
#use RS232(rcv=pin_c7,xmit=pin_c6,baud = 9600,bits=8,parity=N)

void main() {
  /* code */
  
  
  while (TRUE) {
    /* code */
 char op_adc = '\0';
  op_adc = getc();
  
  if(op_adc == 'x'){
  
  printf("TECLEASTE X");
  }
  }
}
